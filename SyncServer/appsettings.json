{
  "Storage": {
    "InboundRoot": "D:/Inbound",
    "TempRoot": "D:/Inbound/.temp",
    "DeleteStrategy": "DeleteDisabled",
    "MaxParallelUploads": 4
  },
  "ApiKeys": {
    "DatasetKeys": {
      "pdf-dataset": "demo-secret-key"
    },
    "ClientKeys": {
      "pc-001": "demo-secret-key"
    }
  },
  "AppLogging": {
    "ApplicationName": "SyncServer",
    "MinimumLevel": "Information",
    "File": {
      "Enabled": true,
      "RetainDays": 14,
      "FileSizeLimitMB": 50
    },
    "Seq": {
      "Enabled": false,
      "ServerUrl": "http://localhost:5341",
      "BufferRelativePath": "seq-buffer",
      "PeriodSeconds": 5
    }
  },
  "AllowedHosts": "*"
}
