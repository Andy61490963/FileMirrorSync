{
  "Storage": {
    "InboundRoot": "D:/Inbound",
    "TempRoot": "D:/Inbound/.temp"
  },
  "ApiKeys": {
    "Keys": {
      "demo": "demo-secret-key"
    }
  },
  "AppLogging": {
    "ApplicationName": "SyncServer",
    "MinimumLevel": "Information",
    "File": {
      "Enabled": true,
      "RetainDays": 14,
      "FileSizeLimitMB": 50
    },
    "Seq": {
      "Enabled": false,
      "ServerUrl": "http://localhost:5341",
      "BufferRelativePath": "seq-buffer",
      "PeriodSeconds": 5
    }
  },
  "AllowedHosts": "*"
}
